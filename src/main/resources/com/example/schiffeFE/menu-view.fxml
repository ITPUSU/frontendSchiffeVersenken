<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="50.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.schiffeFE.MenuController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>

  <Label fx:id="menuText"/>
  <Button text="Create Server" onAction="#onMenuButtonClick"/>
  <Button text="Find Server" onAction="#onMenuButtonClick"/>
  <Button text="Options" onAction="#onMenuButtonClick"/>
  <Button text="Help" onAction="#onMenuButtonClick"/>
  <Button text="Quit Game" cancelButton="true" onAction="#quitGame"/>
</VBox>
